{
  "name": "confectionery-factory-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@neondatabase/serverless": "^1.0.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "express": "^4.18.2",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.4.0",
    "react-scripts": "5.0.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^31.3.1",
    "electron-builder": "^24.13.3",
    "electronmon": "^2.0.2",
    "wait-on": "^7.2.0"
  },
  "scripts": {
    "start": "node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev-client": "react-scripts start",
    "dev-server": "node server.js",
    "electron-start": "concurrently \"cross-env BROWSER=none npm run dev-client\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron-build": "npm run build && electron-builder"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module",
  "main": "main.js",
  "build": {
    "appId": "com.confectionery.factory",
    "productName": "Сладкий Мир",
    "files": [
      "build/**/*",
      "server.js",
      "main.js"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "deb"
    }
  }
}